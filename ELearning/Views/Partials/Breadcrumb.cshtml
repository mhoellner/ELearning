@using ELearning.Models
@model List<ELearning.Models.Item>

<div class="breadcrumb">
    @foreach (Item item in Model)
    {
        if (Model.Last().Id != item.Id)
        {
            <text><a href="@item.Url">@item.Name</a> &rsaquo; </text>
        }
        else
        {
            @item.Name
        }
    }
</div>